{
  "name": "@fzkit/oauth2-clients",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./global-config": {
      "import": {
        "types": "./dist/global-config.d.mts",
        "default": "./dist/global-config.mjs"
      },
      "require": {
        "types": "./dist/global-config.d.ts",
        "default": "./dist/global-config.js"
      }
    },
    "./user-data": {
      "import": {
        "types": "./dist/user-data.d.mts",
        "default": "./dist/user-data.mjs"
      },
      "require": {
        "types": "./dist/user-data.d.ts",
        "default": "./dist/user-data.js"
      }
    },
    "./google": {
      "import": {
        "types": "./dist/clients/google.d.mts",
        "default": "./dist/clients/google.mjs"
      },
      "require": {
        "types": "./dist/clients/google.d.ts",
        "default": "./dist/clients/google.js"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/**/*.ts"
    ],
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "treeshake": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tsup": "~8.3.6"
  },
  "dependencies": {
    "fastify": "~5.2.1",
    "@fzkit/base": "workspace:*",
    "@fastify/oauth2": "~8.1.2",
    "@fastify/cookie": "~11.0.2"
  }
}
