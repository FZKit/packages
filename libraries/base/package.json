{
  "name": "@fzkit/base",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./src/index.ts"
    },
    "./plugin": {
      "source": "./src/plugin.ts",
      "import": {
        "types": "./dist/plugin.d.mts",
        "default": "./dist/plugin.mjs"
      },
      "require": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "default": "./src/plugin.ts"
    },
    "./http-client": {
      "source": "./src/http-client.ts",
      "import": {
        "types": "./dist/http-client.d.mts",
        "default": "./dist/http-client.mjs"
      },
      "require": {
        "types": "./dist/http-client.d.ts",
        "default": "./dist/http-client.js"
      },
      "default": "./src/http-client.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/**/*.ts"
    ],
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "treeshake": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify-plugin": "~5.0.1",
    "fastify": "~5.2.1"
  },
  "devDependencies": {
    "@types/node": "~22.13.1",
    "tsup": "~8.3.6",
    "typescript": "~5.7.3"
  }
}
